//popup
$(function(){
	$(document).ready(function(){
	//$('.link-popup').simplebox();
    $(document.body).append('<div id="fader"></div>');
	pp.node = 0;
	pp.fader = document.getElementById("fader");

	$(pp.fader).css({
		background : "#000",
		width : "100%",
		height : "100%",
		opacity : 0.7,
		position : "fixed",
		left : "-100%",
		top : "-100%",
		zIndex : 1001
	});
	$('.link-popup').click(function(e) {e.stopPropagation(); f_popup($(this).attr("href"));});
	$('#popup .button').click(function(e) {e.stopPropagation(); f_popup("#popup-2");});
	$('#popup-2 .button').click(function(e) {e.stopPropagation(); f_popup("#popup");});
	$('#popup-2 .submit').click(function(e) {e.stopPropagation(); f_popup("#popup-3");});
	$('#popup-3 .button').click(function(e) {e.stopPropagation(); f_popup("#popup-2");});
});
});


var pp = {duration : 300};
function f_popup(el_id) {
	if (pp.node != 0) {
		$(pp.fader).clone().appendTo(document.body).animate({left : "-100%", top : "-100%"}, pp.duration, function() {$(this).remove();});
		$(pp.fader).css({left : "-100%", top : "-100%"});
		$(pp.node).stop().animate({left : -pp.pw+"px", top : -pp.ph+"px"}, pp.duration);
	}

	var i_d = el_id;
	pp.node = document.getElementById(i_d.substr(1,i_d.length-1));
	$(pp.node).click(function(e) {e.stopPropagation();});
	pp.pw = $(pp.node).width();
	pp.ph = $(pp.node).height();
	pp.aw = ($(window).width() - pp.pw)/2;
	pp.ah = ($(window).height() - pp.ph)/2;
	pp.aw = pp.aw > 0 ? pp.aw : 0;
	pp.ah = pp.ah > 0 ? pp.ah : 0;

	$(window).resize(function() {
		pp.pw = $(pp.node).width();
		pp.ph = $(pp.node).height();
		pp.aw = ($(window).width() - pp.pw)/2;
		pp.ah = ($(window).height() - pp.ph)/2;
		pp.aw = pp.aw > 0 ? pp.aw : 0;
		pp.ah = pp.ah > 0 ? pp.ah : 0;
		$(pp.node).css({left : pp.aw+"px", top : pp.ah+"px"});
	});

	$(pp.fader).animate({left : "0%", top : "0%"}, pp.duration);
	$(pp.node).css({
		display : "block",
		position : "fixed",
		left : -pp.pw+"px",
		top : -pp.ph+"px"
	}).animate({left : pp.aw+"px", top : pp.ah+"px"}, pp.duration);

	$(document.body).one("click", function() {hide_pp();});
	$(i_d + " .close").one("click", function() {hide_pp();});
}

function hide_pp() {
	$(pp.fader).stop().animate({left : "-100%", top : "-100%"}, pp.duration);
	$(pp.node).stop().animate({left : -pp.pw+"px", top : -pp.ph+"px"}, pp.duration);
	pp.node = 0;
}


(function($) {
	$.fn.simplebox = function(options) { 
		return new Simplebox(this, options); 
	};
	
	function Simplebox(context, options) { this.init(context, options); };
	
	Simplebox.prototype = {
		options:{},
		init: function (context, options){
			this.options = $.extend({
				duration: 300,
				linkClose: 'a.close, a.btn-close',
				divFader: 'fader',
